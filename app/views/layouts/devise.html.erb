<!DOCTYPE html>
<html>
<head>
  <title>FormManagement</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <div class="container">
    <%= render 'layouts/header' %>
    <% if alert.present?  %>
    <div class="alert alert-dark" role="alert"><%= alert %></div>
    <% end %>

    <% if notice.present? %>
    <div class="alert alert-success" role="alert"> <%= notice %> </div>
    <% end %>


    <div class="row">
      <div class="col-6 offset-3 my-5 pt-5 pb-5 bg-info">
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>



